//"to the power"- 

function toThePower(x.y) {
	return Math.pow(x,y);
}
toThePower(2,3);

//"Plus One" - takes "to the power"(above) and adds to it 1".

const power = toThePower(2,3);
const newSum = plusOne(power);
console.log(newSum);

